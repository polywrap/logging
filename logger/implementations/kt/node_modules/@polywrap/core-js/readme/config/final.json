{
  "doc-snippets": {
    "extract": {
      "include": [
        "./src/**/*.{ts,tsx,json,yaml,txt,md,graphql,cue}",
        "./readme/sub-sections/injected/**/*.{ts,tsx,json,yaml,txt,md,graphql,cue}"
      ],
      "ignore": [
        "./**/node_modules/**",
        "./**/.polywrap/**",
        "./**/__tests__/**"
      ],
      "dir": "./"
    },
    "inject": {
      "dir": "./readme",
      "include": "./README.md"
    },
    "startTokens": [
      {
        "pattern": "$start: ",
        "inline": false
      },
      {
        "pattern": "/* $: {SNIPPET_NAME} */",
        "inline": true
      }
    ],
    "endTokens": [
      {
        "pattern": "$end",
        "inline": false
      },
      {
        "pattern": "/* $ */",
        "inline": true
      }
    ],
    "outputDir": "./"
  }
}
{
  "name": "@polywrap/web3-config-bundle-js",
  "version": "0.12.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/polywrap/javascript-client.git"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rimraf ./build && tsc",
    "build:docs": "yarn doc-snippets combine",
    "lint": "eslint --color -c ../../../.eslintrc.js src/"
  },
  "dependencies": {
    "@polywrap/config-bundle-types-js": "0.12.0",
    "@polywrap/ethereum-wallet-js": "~0.1.0",
    "@polywrap/sys-config-bundle-js": "0.12.0",
    "@polywrap/uri-resolver-extensions-js": "0.12.0",
    "@polywrap/wasm-js": "0.12.0",
    "base64-to-uint8array": "1.0.0"
  },
  "devDependencies": {
    "doc-snippets": "~1.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "doc-snippets": {
    "extract": {
      "include": [
        "./src/**/*.{ts,tsx,json,yaml,txt,md,graphql,cue}"
      ],
      "ignore": [
        "./**/node_modules/**",
        "./**/.polywrap/**",
        "./**/__tests__/**"
      ],
      "dir": "./"
    },
    "inject": {
      "dir": "./readme",
      "include": "./README.md"
    },
    "startTokens": [
      {
        "pattern": "$start: ",
        "inline": false
      },
      {
        "pattern": "/* $: {SNIPPET_NAME} */",
        "inline": true
      }
    ],
    "endTokens": [
      {
        "pattern": "$end",
        "inline": false
      },
      {
        "pattern": "/* $ */",
        "inline": true
      }
    ],
    "outputDir": "./"
  }
}
